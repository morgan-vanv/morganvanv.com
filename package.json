{
  "name": "morganvanv.com",
  "version": "20.1.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "predeploy": "ng build --configuration production --base-href=/ --output-path=dist",
    "prepare-docs": "rm -rf docs && mkdir docs && cp -r dist//browser/* docs/",
    "deploy": "npm run prepare-docs && gh-pages -d docs"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.3",
    "@angular/common": "^20.1.3",
    "@angular/compiler": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@angular/forms": "^20.1.3",
    "@angular/material": "^20.1.3",
    "@angular/platform-browser": "^20.1.3",
    "@angular/router": "^20.1.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.3",
    "@angular/cli": "^20.1.3",
    "@angular/compiler-cli": "^20.1.3",
    "@types/jasmine": "~5.1.0",
    "gh-pages": "^5.0.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
